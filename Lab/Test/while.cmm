int main() {
    int a = 0, b = 0, c = 2, d = 0, f = 3;
    while (a < 10) {
        a = a + 1;
        b = c * f;  // 输出代码：b = #6
        while (d < 10) {
            d = d + 1;
            b = c + 1;  // 输出代码：b = #3
        }
    }
    return 0;
}

