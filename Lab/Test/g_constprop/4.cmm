struct Detail {
    int score;
    int name;
};
struct ScoreClass {
    struct Detail scoreDetail[100];
    int num;
};
int average(struct ScoreClass class) {
    int scoreSum = 0;
    int i = 0;
    int N = class.num;
    while (i < N) {
        scoreSum = class.scoreDetail[i].score + scoreSum;
        i = i + 1;
    }
    return scoreSum / N;
}

int times(int x) {
    int sum = 0;
    int i = 0;
    int N = 10;
    while (i < 10) {
        sum = sum + x;
        i = i + 1;
    }
    return sum / N;  // x
}