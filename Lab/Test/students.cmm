struct Detail {
    int score;
    int name;
};
struct ScoreClass {
    struct Detail scoreDetail[100];
    int num;
};
int average(struct ScoreClass class) {
    int scoreSum = 0;
    int i = 0; 
    int N = class.num;
    write(N);
    while (i < N) {
        scoreSum = class.scoreDetail[i].score + scoreSum;
        write(scoreSum);
        i = i + 1;
    }
    return scoreSum / N;
}
int main() {
    struct ScoreClass classInit;
    int result = 0, initN = 10;
    int j = 0;
    classInit.num = initN;
    write(classInit.num); // 10
    while (j < initN) {
        classInit.scoreDetail[j].score = (j + 1) * (j + 1);
        // write(classInit.scoreDetail[j].score); // 1 4 9 16 25 36 49 64 81 100
        j = j + 1;
    }
    result = average(classInit);
    write(result);
    return 0;
}
